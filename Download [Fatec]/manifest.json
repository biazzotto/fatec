{
  "manifest_version": 2,
  "name": "Download [Fatec]",
  "version": "1.4.15.5",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
       "permissions": [
        "tabs", "<all_urls>"
      ],
      "css": ["sweet/sweetalert.css"],
      "js": ["jquery-2.1.3.min.js","sweet/sweetalert.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "author": "Leonardo Momente",
  "description": "Aquela ajudinha pra baixar os pdfs dos professores hein!",
  "icons": { "16": "icons/icon16.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" }
}